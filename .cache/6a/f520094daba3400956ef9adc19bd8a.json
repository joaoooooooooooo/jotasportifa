ize: 3.5vw;\n  font-family: 'reg';\n\n  list-style: none;\n  margin-top: 10px;\n\n\n}\n\nli {\n  text-decoration: none;\n\n\n  color: #000000;\n}\n\nli:hover {\n  text-decoration: none;\n\n\n  color: #d4d3d3;\n}\n\n\nli:hover {}\n\n.description {\n  font-family: 'reg';\n  font-weight: 500;\n  font-size: 20px;\n  color: #000000;\n\n  letter-spacing: 2%;\n  line-height: 140%;\n}\n\n\n\n@media(max-width: 900px) {\n\n\n  container {\n    background-color: #d4d3d3;\n    border-left: 5px solid #000000;\n\n  }\n\n\n  .description {\n    font-size: 15px;\n    margin-top: 10%;\n  }\n\n  .open {\n    width: 100vw;\n\n\n  }\n\n  .wel {\n    color: #ffa600;\n  }\n\n  #menu_button {\n    padding: 7.5px 15px;\n    font-size: 15px;\n  }\n\n  .purpl {\n    background-color: #d4d3d3;\n    border: none;\n    border-left: 10px solid #d4d3d3;\n    position: absolute;\n    top: 25vw;\n    width: 100vw;\n  }\n\n  .jobs {\n    border: 1px solid #000000;\n    color: #000000;\n    font-size: 15px;\n\n  }\n\n  ul {\n    font-size: 20PX;\n    line-height: 8vw;\n  }\n\n  .list {\n    margin-left: 20px;\n    line-height: 40px;\n    margin-top: 5%;\n  }\n\n\n\n  .title {\n    width: 80%;\n    font-size: 30px;\n  }\n}"},"lineCount":null}},"error":null,"hash":"c30e63c0a5c2ef46beaf234e4ff14101","cacheData":{"env":{}}}